{
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "target": "ES2020",
    "declaration": true,
    "esModuleInterop": true
  }
}
